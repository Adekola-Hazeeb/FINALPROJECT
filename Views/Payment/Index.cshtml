@model ICollection<FINALPROJECT.Domain.Models.ResponseModel.PaymentResponseModel>
@{
    Layout = "_Layout";
}

    <h2>Payments</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Amount</th>
                <th>Date</th>
                <th>Reference ID</th>
                <th>Customer ID</th>
                <th>Auction ID</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var payment in Model)
            {
                <tr>
                    <td>@payment.Amount</td>
                    <td>@payment.Date</td>
                    <td>@payment.ReferenceID</td>
                    <td>@payment.CustomerId</td>
                    <td>@payment.AuctionId</td>
                    <td>@payment.Status</td>
                </tr>
            }
        </tbody>
    </table>
}

